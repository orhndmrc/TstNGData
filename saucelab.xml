<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name = " SauceLabTestSuite" parallel="tests" thread-count="3">
    <test name=" Firefox">


        <parameter name="browser" value="Firefox"></parameter>
        <parameter name="version" value="84.0"></parameter>
        <parameter name="platform" value="WIN10"></parameter>
        <classes>
            <class name="tests.GoogleTestSauceLab"></class>
        </classes>
    </test>
    <test name=" Chrome">

        <parameter name="browser" value="Chrome"></parameter>
        <parameter name="version" value="73.0"></parameter>
        <parameter name="platform" value="WIN10"></parameter>
        <classes>
            <class name="tests.GoogleTestSauceLab"></class>
        </classes>
    </test>
    <test name="Safari">

        <parameter name="browser" value="Safari"></parameter>
        <parameter name="version" value="13.0"></parameter>
        <parameter name="platform" value="macOS 10.13"></parameter>
        <classes>
            <class name="tests.GoogleTestSauceLab"></class>
        </classes>
    </test>

</suite>